@model Department
@{
    ViewData["Title"] = "Department Details";
}

<h1>Department Details</h1>

<form>
    <label asp-for="Code">@Html.DisplayNameFor(D=> D.Code)</label>
    <input asp-for="Code" readonly/>
    <br />
    <label asp-for="Name">@Html.DisplayNameFor(D=>D.Name)</label>
    <input asp-for="Name" readonly/>
    <br />
    <label asp-for="DateCreated">@Html.DisplayNameFor(D => D.DateCreated)</label>
    <input asp-for="DateCreated" readonly/>
</form>
<a  asp-action="Edit" asp-controller="Department" asp-route-id="@Model.ID" class="btn btn">Edit</a>
<a  asp-action="Index" asp-controller="Department" class="btn">Back To Home ya sdeky :)</a>

